<script type="text/javascript">
    var Pluto_22_u25l1n710_13_ = Pluto_22_u25l1n710_13_ || {};

    
        
        
            
            Pluto_22_u25l1n710_13_.jQuery = up.jQuery;
        
    
    
    var upAttachments = upAttachments || {};

    Pluto_22_u25l1n710_13_.jQuery(function () {
        var $ = Pluto_22_u25l1n710_13_.jQuery;

        var open_box = function()
        {
            var $ = Pluto_22_u25l1n710_13_.jQuery;
            $("#Pluto_22_u25l1n710_13_attachments .lb_backdrop, #Pluto_22_u25l1n710_13_attachments .lb_container").animate({'opacity': '.50'}, 300, 'linear');
            $("#Pluto_22_u25l1n710_13_attachments .lb_container").animate({'opacity': '1.00'}, 300, 'linear');
            $("#Pluto_22_u25l1n710_13_attachments .lb_backdrop, #Pluto_22_u25l1n710_13_attachments .lb_container").css('display', 'block');
        }

        var close_box = function()
        {
            var $ = Pluto_22_u25l1n710_13_.jQuery;
            $("#Pluto_22_u25l1n710_13_attachments .lb_backdrop, #Pluto_22_u25l1n710_13_attachments .lb_container").animate({'opacity':'0'}, 300, 'linear', function(){
                $("#Pluto_22_u25l1n710_13_attachments .lb_backdrop, #Pluto_22_u25l1n710_13_attachments .lb_container").css('display', 'none');
            });
        }
        
        if (!upAttachments.show) {
            upAttachments = {
                hide: function() {
                    var $ = Pluto_22_u25l1n710_13_.jQuery;
                    $("#Pluto_22_u25l1n710_13_filename").val('');
                    $("#Pluto_22_u25l1n710_13_source").val('');
                    close_box();
                },
                show: function(callback) {
                    var $ = Pluto_22_u25l1n710_13_.jQuery;
                    var uploader = new qq.FineUploader({
                        element: $("#Pluto_22_u25l1n710_13_file-uploader")[0],
                        template: 'qq-template-gallery',
                        request: {
                            endpoint: "/SimpleContentPortlet/api/content/attach/local.json",
                            forceMultipart: true,
                            paramsInBody: true,
                            params: {
                                filename: function () { return $("#Pluto_22_u25l1n710_13_filename").val(); },
                                source: function () { return $("#Pluto_22_u25l1n710_13_source").val(); }
                            }
                        },

                        multiple: false,

                        validation: {
                            allowedExtensions: [],
                            sizeLimit: 20971520,
                            stopOnFirstInvalidFile: true
                        },

                        autoUpload: false,

                        text: {
                            uploadButton: "Select File",
                            cancelButton: 'Cancel',
                            retryButton: 'Retry',
                            failUpload: 'Upload failed',
                            dragZone: 'Drop files here to upload',
                            formatProgress: "{percent}% of {total_size}",
                            waitingForResponse: "Processing..."
                        },

                        messages: {
                            typeError: "{file} has an invalid extension. Valid extension(s): {extensions}.",
                            sizeError: "{file} is too large, maximum file size is {sizeLimit}.",
                            minSizeError: "{file} is too small, minimum file size is {minSizeLimit}.",
                            emptyError: "{file} is empty, please select files again without it.",
                            noFilesError: "No files to upload.",
                            onLeave: "The files are being uploaded, if you leave now the upload will be cancelled."
                        },

                        debug: true,

                        callbacks: {
                            onSubmit: function (id, filename) {
                                var $ = Pluto_22_u25l1n710_13_.jQuery;
                            },
                            onComplete: function (id, fileName, responseJSON) {
                                var $ = Pluto_22_u25l1n710_13_.jQuery;
                                delete responseJSON.success;
                                callback(responseJSON);
                            },
                            showMessage: function (message) {
                                var $ = Pluto_22_u25l1n710_13_.jQuery;
                                $("#Pluto_22_u25l1n710_13_file-uploader").append('<div class="alert alert-error">' + message + '</div>');
                            }
                        }

                    });

                    $("#Pluto_22_u25l1n710_13_attachments .lb_close, #Pluto_22_u25l1n710_13_attachments .lb_backdrop").click(function () {
                        var $ = Pluto_22_u25l1n710_13_.jQuery;
                        uploader.reset();
                        $("#Pluto_22_u25l1n710_13_filename").val('');
                        close_box();
                    });

                    $("#Pluto_22_u25l1n710_13_triggerUpload").click(function () {
                        uploader.uploadStoredFiles();
                    })

                    open_box();
                }
            };
        }

    });
