  <script type="text/javascript">
    var gearPage = document.getElementById("GearPage");
    if (null != gearPage) {
      gearPage.parentNode.removeChild(gearPage);
      document.title = "Error";
    }

    $(document).ready(function () {
      console.log("Jquery Loaded");
      var facilities = [];

      var unitKey = "1W4J";

      $.ajax({
        type: "GET",
        url: "https://apps.sa.uic.edu/pcs/api/units/current/" + unitKey,
        dataType: "json",
        success: function (response) {
          var records = response;

          $.each(records, function (i, item) {
            facilities.push(item.facId);
          });

          facilities = $.unique(facilities);

          $("#mainDiv").append(
            `<div id="FacAccordian" class="accordian"></div>`
          );

          for (let idx = 0; idx < facilities.length; idx++) {
            const facId = facilities[idx];

            // GET all areas for this fac grp
            var areas = records.filter((r) => r.facId == facId);

            // if there is atleast 1 area create the filter card grp
            if (areas.length > 0) {
              var filterName = areas[0].facName;

              $("#FacAccordian").append(
                `<div id="fac` +
                facId +
                `" class="card">
            <div class="card-header font-weight-bold" data-target="#facBody${facId}" data-toggle="collapse">
              ` +
                filterName +
                `
            </div>
            <div id="facBody${facId}" class="card-body classname collapse">
                <div class="row row-cols-1 row-cols-sm-2 row-cols-md-4">
                </div>
            </div>
          </div>`
              );
            }

            // $(".FacAccordian").collapse({
            //   toggle: true,
            // });
            //  $("#mainDiv #FacAccordian .card .classname").collapse("hide");
            //  $("#mainDiv #FacAccordian .card .classname").css("color","red");

            function displayAvailable(count, max) {
              let rem = count;
              if (rem < 0) rem = 0;
              return rem;
            }

            areas.forEach((area) => {
              const noMessage = area.customMsg === null;
              const countOrMsg =
                noMessage && area.patronCount === null
                  ? "No data"
                  : noMessage
                    ? displayAvailable(area.patronCount, area.maximumAllowed)
                    : area.customMsg;

              $("#mainDiv #fac" + facId + " .card-body .row").append(
                `<div class="col">
                    <div class="card text-center" style="margin-bottom: 15px;">
                      <div class="card-body">
                        <h5 class="card-title area-name">` +
                area.areaName +
                `</h5>
                        <p class="card-text area-status">
                          <h3>` +
                countOrMsg +
                (area.maximumAllowed > 0 && area.maximumAllowed < 10000 ? "</h3><h6>Max: " + area.maximumAllowed : "") +
                "</h6><h6>Last Updated: " +
                (area.recordTime === null ? "-" : area.recordTime) +
                `<h6></p>
                      </div></div>
                    </div>`
              );
            });
          }
        },
        error: function (response, ajaxOptions, thrownError) {
          console.error(thrownError);
        },
      });
    });
  </script>
</body>

</html>
