  <script type="text/javascript">
    var gearPage = document.getElementById("GearPage");
    if (null != gearPage) {
      gearPage.parentNode.removeChild(gearPage);
      document.title = "Error";
    }

    $(document).ready(function () {
      console.log("Jquery Loaded");
      var facilities = [];

      var unitKey = "FD3R";
      //var unitKey = "ADF1";

      $.ajax({
        type: "GET",
        url: "https://dev.sa.uic.edu/pcs/api/units/current/" + unitKey,
        dataType: "json",
        success: function (response) {
          var records = response;

          $.each(records, function (i, item) {
            facilities.push(item.facId);
          });

          facilities = $.unique(facilities);

          for (let idx = 0; idx < facilities.length; idx++) {
            const facId = facilities[idx];

            // GET all areas for this fac grp
            var areas = records.filter((r) => r.facId == facId);

            // if there is atleast 1 area create the filter card grp
            if (areas.length > 0) {
              var filterName = areas[0].facName;

              $("#mainDiv").append(
                `<br/><div id="fac` +
                  facId +
                  `" class="card">
            <div class="card-header">
              ` +
                  filterName +
                  `
            </div>
            <div class="card-body">
                <div class="row">
                </div>
            </div>
          </div><br/>`
              );
            }

            areas.forEach((area) => {
              var displayCount =
                area.patronCount == null
                  ? area.customMsg
                  : area.patronCount + " / " + area.maximumAllowed;

              $("#mainDiv #fac" + facId + " .card-body .row").append(
                `<div class="col"><div class="card text-center">
                <div class="card-body">
              <h5 class="card-title area-name">` +
                  area.areaName +
                  `</h5>
              <p class="card-text area-status"><h3>` +
                  displayCount +
                  ` </h3><br/><h6>Last Updated on: ` +
                  area.recordTime +
                  `<h6></p>
            </div></div>
          </div>`
              );
            });
          }
        },
        error: function (response, ajaxOptions, thrownError) {
          console.error(thrownError);
        },
      });
    });
  </script>
