<script src="https://nexus.admissions.uic.edu/js/app.js" type="text/javascript"></script>
    <script defer src="https://nexus.admissions.uic.edu/plugins/owl/dist/owl.carousel.js"></script>
    <script>
        $(document).ready(function () {

            var owl = $('.owl-carousel');
            owl.owlCarousel({
                items: 1,
                loop: true,
                margin: 0,
                autoplay: true,
                autoplayTimeout: 5000,
                autoplayHoverPause: true,
                dots: true
            });

            $('button.play').on('click', function () {
                owl.trigger('play.owl.autoplay', [1000]);
                $('button.owl-controls').removeClass('active');
                $(this).addClass('active');
            });

            $('button.stop').on('click', function () {
                owl.trigger('stop.owl.autoplay');
                $('button.owl-controls').removeClass('active');
                $(this).addClass('active');
            });

            $('button.next').click(function () {
                owl.trigger('next.owl.carousel', [300]);
            })

            $('button.previous').click(function () {
                owl.trigger('prev.owl.carousel', [300]);
            })

            result = 0;

            $.ajax({
                url: '/api/slate-event',
                method: 'GET',
                success: function (json_response) {

                    var markup = '';

                    for (var i = 0; i < json_response.data.length; i++) {

                        var webinar = json_response.data[i];

                        if(webinar.attributes.invite_only) {
                            continue;
                        }

                        if (i >= 10) {
                            markup += '<tr id="event-' + webinar.id + '" class="more-webinar-premiere on_load" data-sponsor="' + webinar.attributes.sponsor_department + '" data-title="' + webinar.attributes.title + '">';
                        } else {
                            markup += '<tr id="event-' + webinar.id + '" class="on_load" data-sponsor="' + webinar.attributes.sponsor_department + '" data-title="' + webinar.attributes.title + '">';
                        }

                        console.log(webinar.attributes);

                        markup += '<td>' + webinar.attributes.title + '</td>';
                        markup += '<td class="d-none d-sm-table-cell">' + webinar.attributes.sponsor_department + '</td>';
                        markup += '<td class="d-none d-sm-table-cell">' + (webinar.attributes.is_online == true ? 'Online' : 'In-person') + '</td>';
                        markup += '<td data-unix="' + webinar.attributes.start.timestamp + '">' + webinar.attributes.appreciated_form_datestamp + '<br/>' + webinar.attributes.appreciated_form_timestamp + '</td>';
                        if(true == true) { // todo - Is this clause still required?
                            markup += '<td>';
                            markup += '<button type="button" class="btn btn-primary slate-register slate-register-remind" data-slate-id="' + webinar.id + '" data-slate-instance="applynow.uic.edu" data-event_label="'  +  2021 + '_register_'  +  webinar.attributes.start.machine + '_'  +  webinar.attributes.title_machine + '" data-event_category="button" data-event_action="show_register_form">Register</button>';
                            markup += '</td>';
                        } else {
                            markup += '<td></td>';
                        }
                        markup += '</tr>';
                        result++;

                    }

                    $('tbody#webinar-list').html(markup);
                    if(result >= 10) {
                        $('.action.show-hide').show();
                    }

                },
                error: function (error_response) {

                    var json_response = JSON.parse(error_response.responseText);

                    $("#error_message .alert span.lead-error").html(json_response.message);

                    for (const property in json_response.errors) {

                        $('#error_message .alert ul.error-list').append('<li>' + json_response.errors[property][0] + '</li>');

                    }

                    $("#error_message").show();

                }
            });

            $('#search-events').on('keyup', function (event) {

                // console.log('Key');
                // console.log(event.key);

                if (event.key == "Enter") {

                    searchEvents();

                }

                if (event.key == "Escape") {

                    searchEventsReset();

                }
            });

            $('button#search-events-reset').click(function () {

                searchEventsReset();

            });

            $('button#search-events-submit').click(function () {

                searchEvents();

            });

            $(".show-hide").on("click", function () {
                $('tr.on_load').removeClass('on_load');
                var txt = $("#schedule .more-webinar-premiere").is(':visible') ? 'Show more webinars' : 'Show less webinars';
                $(".show-hide").text(txt);
                $('#schedule .more-webinar-premiere').slideToggle();
            });


            $('iframe').each(function (index, iframe) {

                // Adjust any Iframes to be at 100% with 16x9 ratio on any content page
                var r = $(this).parent("div").width();
                var a = 10 * r / 16;

                $(this).css("width", r + "px");
                $(this).css("height", a + "px");
            });

            $('body').on('click', '#main-front-page-cta', function () {
                $('html,body').animate({scrollTop: $('#show-more-wrap').offset().top - 160}, 'fast');
            });

            $('body').on('click', '#tour-campus-button', function () {
                $('html,body').animate({scrollTop: $('#social-media').offset().top}, 'fast');
            });

            $("#chat").addClass('on-screen-focus');
            $('.faux-button.close').click(function () {
                $("#chat").removeClass('on-screen-focus').addClass('on-screen-compact');
            });
            $('body').on('click', '.slate-register', function () {

                if ($(this).data('slateId') && $(this).data('slateInstance')) {

                    let url = 'https://' + $(this).data('slateInstance') + '/register/?id=' + $(this).data('slateId');

                    if(_GET('person') != null) {


                        url = url + '&person=' + _GET('person');

                    }

                    window.open(
                        url,
                        '_blank'
                    );

                }

            });

            var table = $('table#schedule');

            $('#feature_header, #host_header')
                .wrapInner('<span title="sort this column"/>')
                .each(function () {

                    var th = $(this),
                        thIndex = th.index(),
                        inverse = false;

                    th.click(function () {

                        table.find('td').filter(function () {

                            return $(this).index() === thIndex;

                        }).sortElements(function (a, b) {

                            return $.text([a]) > $.text([b]) ?
                                inverse ? -1 : 1
                                : inverse ? 1 : -1;

                        }, function () {

                            // parentNode is the element we want to move
                            return this.parentNode;

                        });

                        inverse = !inverse;

                    });

                });

            $('#date_header')
                .wrapInner('<span title="sort this column"/>')
                .each(function () {

                    var th = $(this),
                        thIndex = th.index(),
                        inverse = false;

                    th.click(function () {

                        table.find('td').filter(function () {

                            return $(this).index() === thIndex;

                        }).sortElements(function (a, b) {

                            return $(a).data('unix') > $(b).data('unix') ?
                                // return $.text([a]) > $.text([b]) ?
                                inverse ? -1 : 1
                                : inverse ? 1 : -1;

                        }, function () {

                            // parentNode is the element we want to move
                            return this.parentNode;

                        });

                        inverse = !inverse;

                    });

                });


        });

        function killSlateProgressBox() {

            $('.progress_div').remove();
            $('.progress_box').remove();

            console.log('removes_slate_bleed');

            // Remove any existing .slate_form
            $('.slate_form').remove();

            $('body').removeClass('progress');
        }

        function searchEvents() {

            query = $('input#search-events').val();

            $('tbody#webinar-list tr').each(function (index, element) {

                // Check query against Title, College, Sponsor, and Department
                if ($(element).attr('data-title').toLowerCase().indexOf(query.toString().toLowerCase()) >= 0 ||
                    $(element).attr('data-sponsor').toLowerCase().indexOf(query.toString().toLowerCase()) >= 0
                ) {

                    $(element).show();
                    result++;

                } else {

                    $(element).hide();

                }

            });

        }

        function searchEventsReset() {

            count = 0;

            $('tbody#webinar-list tr').each(function (index, element) {

                if (count <= 9) {

                    $(this).show();

                } else {

                    $(this).hide();

                }

                count++;

            });
            $('input#search-events').val('');

        }

        /**
         * jQuery.fn.sortElements
         * --------------
         * @author  James Padolsey (http://james.padolsey.com)
         * @version  0.11
         * @updated  18-MAR-2010
         * --------------
         * @param  Function comparator:
         *   Exactly the same behaviour as [1,2,3].sort(comparator)
         *
         * @param  Function getSortable
         *   A function that should return the element that is
         *   to be sorted. The comparator will run on the
         *   current collection, but you may want the actual
         *   resulting sort to occur on a parent or another
         *   associated element.
         *
         *   E.g. $('td').sortElements(comparator, function(){
         *      return this.parentNode;
         *   })
         *
         *   The <td>'s parent (<tr>) will be sorted instead
         *   of the <td> itself.
         */
        jQuery.fn.sortElements = (function () {

            var sort = [].sort;

            return function (comparator, getSortable) {

                getSortable = getSortable || function () {
                    return this;
                };

                var placements = this.map(function () {

                    var sortElement = getSortable.call(this),
                        parentNode = sortElement.parentNode,

                        // Since the element itself will change position, we have
                        // to have some way of storing it's original position in
                        // the DOM. The easiest way is to have a 'flag' node:
                        nextSibling = parentNode.insertBefore(
                            document.createTextNode(''),
                            sortElement.nextSibling
                        );

                    return function () {

                        if (parentNode === this) {
                            throw new Error(
                                "You can't sort elements if any one is a descendant of another."
                            );
                        }

                        // Insert before flag:
                        parentNode.insertBefore(this, nextSibling);
                        // Remove flag:
                        parentNode.removeChild(nextSibling);

                    };

                });

                return sort.call(this, comparator).each(function (i) {
                    placements[i].call(getSortable.call(this));
                });

            };

        })();

        // https://stackoverflow.com/questions/901115/how-can-i-get-query-string-values-in-javascript
        function _GET(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }

        // https://stackoverflow.com/questions/4656843/jquery-get-querystring-from-url
        function getUrlVars()
        {
            var vars = [], hash;
            var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
            for(var i = 0; i < hashes.length; i++)
            {
                hash = hashes[i].split('=');
                vars.push(hash[0]);
                vars[hash[0]] = hash[1];
            }
            return vars;
        }

    </script>








