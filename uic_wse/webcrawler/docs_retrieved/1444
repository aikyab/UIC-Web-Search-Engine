            function validate(){
                if(document.survey_7074059_1201T230311.isValidate.value == 'true'){
                    document.survey_7074059_1201T230311.isValidate.value='false';
                    document.survey_7074059_1201T230311.USER_userId.value=document.survey_7074059_1201T230311.USER_userId_for_save.value;
                    if(document.survey_7074059_1201T230311.USER_userId.value == ''){
                        alert('Please enter your email address and try again.');
                        return false;
                    }else{
                        if(confirm('This will SAVE your results and send you an email to finish later.  Your results will not be submitted until you return and click Submit.  Continue with Save?') == true){
                            return true;
                        }else{
                            return false;
                        }
                    }
                }
            }
        </script><div id="dskCont">
