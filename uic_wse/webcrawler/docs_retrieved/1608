marker_markermap_105.bindPopup("<div class='popup-directions' style=''>Mt. Vernon Illinois, United States of America (<a href=http://maps.google.com/maps?daddr=Mt.+Vernon+Illinois%2C+United+States+of+America&t=m&layer=1&doflg=ptk&om=0 target='_blank' title='Get directions'>Directions</a>)</div>", {maxWidth: 300, minWidth: 250, maxHeight: 160, autoPan: true, closeButton: true, autoPanPadding: new L.Point(5, 5)});
})(jQuery);
jQuery(document).ready( function($) {
function resizeMap() {
	var map = jQuery('#lmm_0239dd30');
		var map_parent_size = jQuery('#lmm_0239dd30').parent().width();
	if( (map_parent_size >0) && (map_parent_size < 640) ) {
		map.css({ 'width': '100%'});
		markermap_105.invalidateSize();
	}
}
resizeMap();
if (typeof jQuery().modal == 'function') {
		markermap_105.invalidateSize();
		jQuery('a[data-toggle="tab"],.tabbed-area a,.nav-tabs a').on('shown.bs.tab', function (e) {
			markermap_105.invalidateSize();
		});
	}
	if (typeof jQuery.ui != 'undefined') {
	markermap_105.invalidateSize();
	jQuery('.ui-tabs').on('tabsactivate', function(event, ui) {
		markermap_105.invalidateSize();
	});
}
if (typeof jQuery.mobile != 'undefined') {
	jQuery(document).bind('pageinit', function( event, data ){
		markermap_105.invalidateSize();
	});
}
});

/* ]]> */
</script>
