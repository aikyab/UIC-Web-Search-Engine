<script type="text/javascript">
// check to see if there are any links to other sites before continuing

if (jQuery('h4 a').not('[href*="jobs.uic.edu"]').length > 0) {
    
    // selects all the li elements that contain links to other sites
    var nonUICJobs = jQuery('h4 a').not('[href*="jobs.uic.edu"]').parent().parent()

    // creates a copy of one of the headers as a template for our new header
    var newHeader = jQuery('ul.title_header .title_subheader').eq(0).clone()

    /* appends the cloned header, removes any li elements, modifies the text, and 
    appends the li elements for other sites */

    jQuery('.title_header').append(newHeader)
    newHeader.children('ul').children('li').remove()
    newHeader.children('h2').text('University of Illinois Springfield, System Office,\
                                 Urbana-Champaign, and UI Foundation')
    newHeader.children('ul').eq(0).append(nonUICJobs)

    // sorts the job list alphabetically 
    sortChildElements(newHeader.children('ul').get(0))

    // some headers on the page are now empty, removing them
    jQuery('li.title_subheader ul:not(:has(li))').parent().remove()                                 
}

function sortChildElements(list) {
    var i, switching, b, shouldSwitch;
    switching = true;
    /*Make a loop that will continue until
    no switching has been done:*/
    while (switching) {
      //start by saying: no switching is done:
      switching = false;
      //break if no child elements
      if (list.children === undefined) { break }
      b = list.children;
      //Loop through all child elements:
      for (i = 0; i < (b.length - 1); i++) {
        //start by saying there should be no switching:
        shouldSwitch = false;
        /*check if the next item should
        switch place with the current item:*/
        if (b[i].textContent.toLowerCase() > b[i + 1].textContent.toLowerCase()) {
          /*if next item is alphabetically
          lower than current item, mark as a switch
          and break the loop:*/
          shouldSwitch = true;
          break;
        }
      }
      if (shouldSwitch) {
        /*If a switch has been marked, make the switch
        and mark the switch as done:*/
        b[i].parentNode.insertBefore(b[i + 1], b[i]);
        switching = true;
      }
    }
  }
</script>
